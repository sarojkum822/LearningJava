package patternmatching;

public class pattern {

	public static void main(String[] args) {
			int i;
			int j;
			int k;
			int n=5;
			for(i=1;i<=n;i++){
				for(j=1;j<=n;j++) 
					{
					if(i==1 || i==5 || j==1 || j==5) {
						System.out.println("*");
					}
					else {
					System.out.print("-");
				}
				System.out.println();
			}
			
	}

}
