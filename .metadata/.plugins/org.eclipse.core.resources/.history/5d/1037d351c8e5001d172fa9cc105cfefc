package patternmatching;

public class traingle {
	
	public static void main(String[] args) {
		int i;
		int j;
		int k;
		int n=5;
		for(i=1;i<=n;i++) {
			for(k=1;k<=n-i;k++) {
				System.out.print(" ");
			}
			for(j=1;j<=i;j++) {
				if(j==i || i==5 || j==1)
				System.out.print("1 ");
			}
			System.out.println();
		}
	}

}
